@model HomeTaskOne.Models.PageModels.HomePageModel

@{
    ViewData["StyleSheet"] = "ArticlesStyles.css";
    Layout = "~/Views/Shared/BasePageWrapper.cshtml";
}

@foreach(HomeTaskOne.Models.PageItemModels.Article article in Model.Articles) 
{
    <div class="article-wrapper">
        <div class="article-title">@article.Title</div>
        <div class="article-text">@article.Body</div>
    </div>
}